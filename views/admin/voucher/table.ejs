<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama Game</th>
      <th>Category</th>
      <th>Nominal</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
        <% for( let i = 0; i < voucher.length; i++ ) { %>
            <tr>
                <td><%= i + 1 %></td>
                <td><%= voucher[i].name %></td>
                <td><%= voucher[i].category.name %></td>
                <td>
                    <% voucher[i].nominals.forEach(nominal => { %>
                        <span class="badge badge-warning">
                            <%= nominal.coinName %><%= nominal.coinQuantity %>  
                        </span>
                    <% }) %>
                </td>
                <td>
                    <!-- kondisi voucher status -->
                    
                </td>
                <td>
                    <form action="/voucher/status/<%= voucher[i]._id %>?_method=PUT" method="POST" style="display: inline-block">
                        <!-- kondisi button status -->
                        
                    </form>
                    <form style="display: inline-block" action="/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="post">
                        <a href="/voucher/edit/<%= voucher[i]._id %>" class="btn btn-warning"><i class="fas fa-pencil-alt"></i> Ubah</a>
                        <button class="btn btn-danger"><i class="fas fa-trash-alt"></i> Hapus</button>
                    </form>
                </td>
               
            </tr>
        <% } %>
    </tbody>
    
  </table>